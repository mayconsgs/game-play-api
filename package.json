{
  "name": "template-express-knex",
  "version": "0.0.0",
  "description": "Template para API Node com Knex",
  "main": "src/server.ts",
  "repository": "https://github.com/mayconsgs/template-api-node-knex.git",
  "author": "Maycon Santos <maycon.s.santos44@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "prestart": "NODE_ENV=production knex migrate:latest",
    "start": "NODE_ENV=production node dist/server.js",
    "predev": "NODE_ENV=development knex migrate:latest",
    "dev": "NODE_ENV=development ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify src/server.ts",
    "pretest": "NODE_ENV=test knex migrate:latest",
    "test": "NODE_ENV=test jest --forceExit --detectOpenHandles"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "joi": "^17.4.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.7",
    "pg": "^8.6.0",
    "sqlite3": "^5.0.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/node": "^7.14.7",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/faker": "^5.5.7",
    "@types/jest": "^26.0.24",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.1",
    "babel-jest": "^27.0.6",
    "babel-plugin-module-resolver": "^4.1.0",
    "faker": "^5.5.3",
    "jest": "^27.0.6",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.1.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  }
}
